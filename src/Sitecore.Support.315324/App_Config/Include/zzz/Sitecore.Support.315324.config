<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/" xmlns:set="http://www.sitecore.net/xmlconfig/set/" xmlns:role="http://www.sitecore.net/xmlconfig/role/">
  <sitecore role:require="Standalone OR ContentManagement OR ContentDelivery OR Processing OR Reporting">
    <pipelines>
      <mvc.getXmlBasedLayoutDefinition>
        <processor patch:instead="*[@type='Sitecore.XA.Feature.Composites.Pipelines.GetXmlBasedLayoutDefinition.InjectCompositeComponents, Sitecore.XA.Feature.Composites']" type="Sitecore.Support.XA.Feature.Composites.Pipelines.GetXmlBasedLayoutDefinition.InjectCompositeComponents, Sitecore.Support.315324" resolve="true"/>
        <processor patch:instead="*[@type='Sitecore.XA.Feature.Composites.Pipelines.GetXmlBasedLayoutDefinition.SetOnPageEditingContext, Sitecore.XA.Feature.Composites']" type="Sitecore.Support.XA.Feature.Composites.Pipelines.GetXmlBasedLayoutDefinition.SetOnPageEditingContext, Sitecore.Support.315324" />
      </mvc.getXmlBasedLayoutDefinition>
      <mvc.getCompositeXmlBasedLayoutDefinition>
        <processor patch:instead="*[@type='Sitecore.XA.Feature.Composites.Pipelines.GetXmlBasedLayoutDefinition.InjectCompositeComponents, Sitecore.XA.Feature.Composites']" type="Sitecore.Support.XA.Feature.Composites.Pipelines.GetXmlBasedLayoutDefinition.InjectCompositeComponents, Sitecore.Support.315324" resolve="true"/>
